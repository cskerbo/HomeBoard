<div class="row">
  <div class="col-md-12">
    <div class="card">
      <div class="card-header card-header-primary">
        <h3 class="card-title ">Existing Lists</h3>
      </div>
      <div class="card-body">
        <div class="table-responsive">
          <table class="table">
            <thead class=" text-primary">
            <th>ID</th>
            <th>Name</th>
            </thead>
            <tbody>
            <% @home.lists.each do |list| %>
              <tr>
                <td><%= link_to list.id, user_home_list_path(current_user, @home.id, list.id), data: { turbolinks: 'false' } %></td>
                <td><%= link_to list.name, user_home_list_path(current_user, @home.id, list.id), data: { turbolinks: 'false' } %></td>
              </tr>
            <%end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>