<div class="content">
  <div class="container-fluid">
    <h2>User List</h2>
    <ul>
      <% @users.each do |user| %>
      <li><%= user.email %></li>
        <%end %>
    </ul>
    <h2>User Home Count</h2>
    <ul>
      <% @users.each do |user| %>
        <li><%= user.email %></li>
        <ul>
          <li><%= user.homes.count %></li>
        </ul>
      <%end %>
    </ul>
    <h2>User with most homes</h2>

    <ul>
        <li><%= @users.most_homes[0].email %></li>
        <ul>
          <li><%= @users.most_homes[0].homes.count %></li>
        </ul>
    </ul>
  </div>
</div>